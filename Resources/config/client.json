{
    "name": "Janus client",
    "description": "Janus manages ICE user accounts",
    "operations": {
        "GetUser": {
            "httpMethod": "GET",
            "uri": "users/{username}",
            "summary": "Get a user",
            "responseClass": "Ice\\JanusClientBundle\\Entity\\User",
            "parameters": {
                "username": {
                    "location": "uri",
                    "type": "string",
                    "required": true
                }
            }
        },
        "CreateUser": {
            "httpMethod": "POST",
            "uri": "users",
            "summary": "Create a user",
            "responseClass": "Ice\\JanusClientBundle\\Entity\\User",
            "errorResponses": [
                {
                    "code":400,
                    "phrase":"Bad request",
                    "class": "Ice\\JanusClientBundle\\Response\\FormError"
                }
            ],
            "parameters": {
                "plainPassword": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "email": {
                    "location": "json",
                    "type": "string",
                    "required": false
                },
                "title": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "firstNames": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "middleNames": {
                    "location": "json",
                    "type": "string",
                    "required": false
                },
                "lastNames": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "dob": {
                    "location": "json",
                    "type": "string",
                    "required": true
                }
            }
        },
        "UpdateUser": {
            "httpMethod": "PUT",
            "uri": "users/{username}",
            "summary": "Update a user",
            "responseClass": "Ice\\JanusClientBundle\\Entity\\User",
            "parameters": {
                "username": {
                    "location": "uri",
                    "type": "string",
                    "required": true
                },
                "email": {
                    "location": "json",
                    "type": "string",
                    "required": false
                },
                "title": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "firstNames": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "middleNames": {
                    "location": "json",
                    "type": "string",
                    "required": false
                },
                "lastNames": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "dob": {
                    "location": "json",
                    "type": "string",
                    "required": true
                }
            }
        },
        "UpdateAttribute": {
            "httpMethod": "PUT",
            "uri": "users/{username}/attributes/{attributeName}",
            "summary": "Set a User Attribute",
            "responseClass": "Ice\\JanusClientBundle\\Entity\\Attribute",
            "parameters": {
                "username": {
                    "location": "uri",
                    "type": "string",
                    "required": true
                },
                "attributeName": {
                    "location": "uri",
                    "type": "string",
                    "required": true
                },
                "value": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "updatedBy": {
                    "location": "json",
                    "type": "string",
                    "required": true
                }
            }
        },
        "CreateAttribute": {
            "httpMethod": "POST",
            "uri": "users/{username}/attributes",
            "summary": "Create a new User Attribute",
            "responseClass": "Ice\\JanusClientBundle\\Entity\\Attribute",
            "parameters": {
                "username": {
                    "location": "uri",
                    "type": "string",
                    "required": true
                },
                "fieldName": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "value": {
                    "location": "json",
                    "type": "string",
                    "required": true
                }
            }
        },
        "GetUsers": {
            "httpMethod": "GET",
            "uri": "users{?query*}",
            "summary": "Get Users with optional filtering",
            "responseClass": "ArrayCollection<Ice\\JanusClientBundle\\Entity\\User>",
            "parameters": {
                "query": {
                    "location": "uri",
                    "type": "array",
                    "required": "false"
                }
            }
        }
    }
}